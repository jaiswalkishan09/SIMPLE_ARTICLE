tion Login() {\n  const [username, setUsername] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const [token, setToken] = useCookies([\"mytoken\"]);\n  const [isLogin, setLogin] = useState(true);\n  let history = useHistory();\n\n  useEffect(() => {\n    if (token[\"mytoken\"]) {\n      history.push(\"/articles\");\n    }\n  }, [token]);\n\n  const loginBtn = () => {\n    if (username.length > 0 && password.length > 0) {\n      APIService.LoginUser({ username, password }).then((resp) => {\n        if (Boolean(resp)) {\n          setToken(\"mytoken\", resp.token);\n          sessionStorage.setItem(\"is_staff\", resp.is_staff);\n        }\n      });\n    } else {\n      alert(\"Please Enter Username and Password\");\n    }\n  };\n\n  const RegisterBtn = () => {\n    APIService.RegisterUser({ username, password })\n      .then((resp) => {\n       if (Boolean(resp)) {\n\n          loginBtn();\n        } \n      })\n  };\n  return (\n    <div className=\"App\">\n      <br />\n      <br />\n      {isLogin ? <h1>Please Login </h1> : <h1>Please Register </h1>}\n\n      <br />\n      <br />\n\n      <div className=\"mb-3\">\n        <label htmlFor=\"username\" className=\"form-label\">\n          Username\n        </label>\n        <input\n          required\n          type=\"text\"\n          className=\"form-control\"\n          id=\"username\"\n          placeholder=\"Please Enter Username\"\n          value={username}\n          onChange={(e) => setUsername(e.target.value)}\n        />\n      </div>\n\n      <div className=\"mb-3\">\n        <label htmlFor=\"password\" className=\"form-label\">\n          Password\n        </label>\n        <input\n          required\n          type=\"password\"\n          className=\"form-control\"\n          id=\"password\"\n          placeholder=\"Please Enter Password\"\n          value={password}\n          onChange={(e) => setPassword(e.target.value)}\n        />\n      </div>\n\n      {isLogin ? (\n        <button onClick={loginBtn} className=\"btn btn-primary\">\n          Login\n        </button>\n      ) : (\n        <button onClick={RegisterBtn} className=\"btn btn-primary\">\n          Register\n        </button>\n      )}\n\n      <div className=\"mb-3\">\n        <br />\n        {isLogin ? (\n          <h5>\n            If You Don't Have Account, Please{\" \"}\n            <button className=\"btn btn-primary\" onClick={() => setLogin(false)}>\n              Register\n            </button>\n            Here\n          </h5>\n        ) : (\n          <h5>\n            If You Have Account, Please{\" \"}\n            <button className=\"btn btn-primary\" onClick={() => setLogin(true)}>\n              Login\n            </button>\n            Here\n          </h5>\n        )}\n      </div>\n    </div>\n  );\n}\n\nexport default Login;\n","/home/kishan/Desktop/React_django/SIMPLE_ARTICLE_React_Django/ReactFrontEnd/my-app/src/components/ArticleList.js",["95","96"],"import React, { useState, useEffect } from \"react\";\nimport APIService from \"../APIService\";\nimport { useCookies } from \"react-cookie\";\n\nfunction ArticleList(props) {\n  const [token] = useCookies([\"mytoken\"]);\n  let is_staff = JSON.parse(sessionStorage?.getItem(\"is_staff\"));\n\n  const editBtn = (article) => {\n    props.editBtn(article);\n  };\n\n  const deleteBtn = (article) => {\n    APIService.DeleteArticle(article.id, token[\"mytoken\"])\n      .then(() => props.deleteBtn(article))\n      .catch((error) => console.log(error));\n  };\n\n  const getUpdatDeletebutton = (article) => {\n\n    if (is_staff ) {\n      return (\n        <div className=\"row\">\n          <div className=\"col-md-1\">\n            <button\n              className=\"btn btn-primary\"\n              onClick={() => editBtn(article)}\n            >\n              Edit\n            </button>\n          </div>\n\n          <div className=\"col\">\n            <button\n              onClick={() => deleteBtn(article)}\n              className=\"btn btn-danger\"\n            >\n              Delete\n            </button>\n          </div>\n        </div>\n      );\n    }\n    return;\n  };\n\n  return (\n    <div>\n      {props.articles &&\n        props.articles.map((article) => {\n          return (\n            <div key={article.id}>\n              <h2 className=\"text-success\">{article.title}</h2>\n              <p>{article.description}</p>\n              {getUpdatDeletebutton(article)}\n\n              <hr className=\"hrclass\" />\n            </div>\n          )\n        })}\n    </div>\n  );\n}\n\nexport default ArticleList;\n","/home/kishan/Desktop/React_django/SIMPLE_ARTICLE_React_Django/ReactFrontEnd/my-app/src/components/Form.js",[],"/home/kishan/Desktop/React_django/SIMPLE_ARTICLE_React_Django/ReactFrontEnd/my-app/src/APIService.js",[],"/home/kishan/Desktop/React_django/SIMPLE_ARTICLE_React_Django/ReactFrontEnd/my-app/src/components/Admin.js",["97","98","99","100","101"],"/home/kishan/Desktop/React_django/SIMPLE_ARTICLE_React_Django/ReactFrontEnd/my-app/src/components/UserList.js",[],"/home/kishan/Desktop/React_django/SIMPLE_ARTICLE_React_Django/ReactFrontEnd/my-app/src/components/UserForm.js",[],"/home/kishan/Desktop/React_django/SIMPLE_ARTICLE_React_Django/ReactFrontEnd/my-app/src/APIUserService.js",[],{"ruleId":"102","severity":1,"message":"103","line":1,"column":8,"nodeType":"104","messageId":"105","endLine":1,"endColumn":12},{"ruleId":"102","severity":1,"message":"106","line":15,"column":17,"nodeType":"104","messageId":"105","endLine":15,"endColumn":25},{"ruleId":"102","severity":1,"message":"107","line":16,"column":10,"nodeType":"104","messageId":"105","endLine":16,"endColumn":21},{"ruleId":"102","severity":1,"message":"108","line":16,"column":23,"nodeType":"104","messageId":"105","endLine":16,"endColumn":37},{"ruleId":"109","severity":1,"message":"110","line":34,"column":6,"nodeType":"111","endLine":34,"endColumn":23,"suggestions":"112"},{"ruleId":"109","severity"